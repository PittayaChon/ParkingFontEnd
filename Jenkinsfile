    environment {
    registryCredential = ''
    dockerImage = ''
  }
  agent  any
    stages {
        stage('clone git') {
            steps {
                    echo 'Building..'
            }
        }
        stage('build image') {
            steps {
                    echo 'Building..'
            }
        }

        stage('push image') {
            steps {
                    echo 'Building..'
            }
        }

        stage('prepare deploy') {
            steps {
                    echo 'Building..'
            }
        }
        stage('deploy') {
            steps {
              script {
                    echo 'Building..'
                }
              }
            }
        }
    }
